#!/usr/bin/env node
var benchmark = require('benchmark')
var nanoid = require('./')
var generate = require('./generate')

var suite = new benchmark.Suite()

suite
  .add('nanoid#generate', function () {
    generate('1234567890abcdef', 10)
    return nanoid()
  })
  .on('cycle', function (event) {
    console.log(String(event.target))
  })
  .run()
