{
  "name": "nanoid",
  "version": "1.3.1",
  "description": "A tiny (143 bytes), secure URL-friendly unique string ID generator",
  "keywords": [
    "uuid",
    "random",
    "id",
    "url"
  ],
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "ai/nanoid",
  "browser": {
    "./index.js": "./index.browser.js",
    "./random.js": "./random.browser.js",
    "./async/index.js": "./async/index.browser.js",
    "./async/random.js": "./async/random.browser.js"
  },
  "devDependencies": {
    "benchmark": "^2.1.4",
    "chalk": "^2.4.1",
    "clean-publish": "^1.0.10",
    "eslint": "^5.7.0",
    "eslint-ci": "^0.1.1",
    "eslint-config-logux": "^26.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-es5": "^1.3.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.25.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.0",
    "gzip-size": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jsdoc": "^3.5.5",
    "lint-staged": "^7.3.0",
    "microtime": "^2.1.8",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.2",
    "rndm": "^1.2.0",
    "shortid": "^2.2.13",
    "size-limit": "^0.20.0",
    "uid-safe": "^2.1.5",
    "uuid": "^3.3.2",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "yaspeller-ci": "^1.0.0"
  },
  "scripts": {
    "lint-staged": "lint-staged",
    "spellcheck": "yarn docs && yaspeller-ci *.md docs/*.html",
    "clean": "rimraf docs/ coverage/",
    "docs": "jsdoc -d docs *.js async/*.js",
    "lint": "eslint-ci *.js async/*.js test/{benchmark,alphabet} test/*.js test/**/*.js",
    "test": "jest --coverage && yarn lint && size-limit && yarn spellcheck",
    "start": "webpack-dev-server --config test/demo/webpack.config"
  },
  "size-limit": [
    {
      "path": "index.js",
      "limit": "142 B"
    },
    {
      "path": "generate.js",
      "limit": "177 B"
    },
    {
      "path": "non-secure.js",
      "limit": "110 B"
    },
    {
      "path": "async/index.js",
      "limit": "154 B"
    },
    {
      "path": "async/generate.js",
      "limit": "198 B"
    }
  ],
  "eslintConfig": {
    "extends": "eslint-config-logux/browser",
    "rules": {
      "consistent-return": "off",
      "yoda": "off"
    },
    "overrides": {
      "files": [
        "test/*.js"
      ],
      "rules": {
        "handle-callback-err": "off",
        "global-require": "off"
      }
    }
  },
  "lint-staged": {
    "*.md": "yaspeller-ci",
    "*.js": "eslint"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "jest": {
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "statements": 100
      }
    }
  },
  "yaspeller": {
    "lang": "en",
    "ignoreCapitalization": true,
    "excludeFiles": [
      "docs/*.js.html",
      "README-*.md"
    ],
    "dictionary": [
      "Versioning",
      "JSDoc",
      "GMT",
      "js",
      "UUID",
      "nanoid",
      "gzipped",
      "Crypto",
      "bruteforcing",
      "cryptographically",
      "v4",
      "Vsevolod",
      "Rodionov",
      "Mokhov",
      "Anton",
      "Khlynovskiy",
      "Klebanoff",
      "IE 11",
      "Haskell",
      "Zachary",
      "Golba",
      "CLI",
      "npm",
      "Clojure",
      "ClojureScript",
      "async",
      "nonSecure",
      "Wenliang",
      "Dai",
      "Ori",
      "Livni",
      "Mon",
      "Tue",
      "Thu",
      "Fri",
      "Eatall",
      "Sep",
      "Hendry",
      "Sadrak",
      "Dmitriy",
      "Tsvettsikh",
      "Belym",
      "Oct",
      "Nim",
      "rqrqrqrq",
      "formatAsync",
      "generateAsync",
      "asyncGenerator",
      "gzip",
      "Daniil",
      "Poroshin",
      "joelgetaction"
    ]
  }
}
