{
  "name": "nanoid",
  "version": "2.0.3",
  "description": "A tiny (141 bytes), secure URL-friendly unique string ID generator",
  "keywords": [
    "uuid",
    "random",
    "id",
    "url"
  ],
  "scripts": {
    "api": "documentation build *.js {async,non-secure}/*.js -f md -o api.md",
    "spell": "yarn api && yaspeller-ci *.md",
    "clean": "rimraf api.md coverage/",
    "test": "jest --coverage && eslint-ci . && size-limit && yarn spell",
    "start": "parcel serve test/demo/index.html -d test/demo/build --open"
  },
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "ai/nanoid",
  "browser": {
    "./index.js": "./index.browser.js",
    "./random.js": "./random.browser.js",
    "./async/index.js": "./async/index.browser.js",
    "./async/random.js": "./async/random.browser.js"
  },
  "react-native": {
    "./async/random.js": "./async/random.reactnative.js"
  },
  "sideEffects": false,
  "devDependencies": {
    "@logux/eslint-config": "^30.0.2",
    "@logux/sharec-config": "^0.4.0",
    "@size-limit/preset-small-lib": "^2.0.0",
    "benchmark": "^2.1.4",
    "chalk": "^2.4.2",
    "clean-publish": "^1.1.2",
    "documentation": "^12.1.1",
    "eslint": "^6.1.0",
    "eslint-ci": "^1.0.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-es5": "^1.4.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prefer-let": "^1.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.0",
    "gzip-size": "^5.1.1",
    "husky": "^3.0.3",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.1",
    "microtime": "^3.0.0",
    "parcel-bundler": "^1.12.3",
    "rimraf": "^2.6.3",
    "rndm": "^1.2.0",
    "secure-random-string": "^1.1.2",
    "shortid": "^2.2.14",
    "uid-safe": "^2.1.5",
    "uuid": "^3.3.2",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "yaspeller-ci": "^1.0.1"
  },
  "size-limit": [
    {
      "path": "index.js",
      "limit": "141 B"
    },
    {
      "path": "generate.js",
      "limit": "180 B"
    },
    {
      "path": "url.js",
      "limit": "64 B"
    },
    {
      "path": "non-secure/index.js",
      "limit": "105 B"
    },
    {
      "path": "non-secure/generate.js",
      "limit": "47 B"
    },
    {
      "path": "async/index.js",
      "limit": "153 B"
    },
    {
      "path": "async/generate.js",
      "limit": "200 B"
    }
  ],
  "eslintConfig": {
    "extends": "@logux/eslint-config/browser",
    "rules": {
      "consistent-return": "off",
      "yoda": "off"
    }
  },
  "eslintIgnore": [
    "node_modules"
  ],
  "lint-staged": {
    "*.md": "yaspeller-ci",
    "*.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "statements": 100
      }
    }
  },
  "yaspeller": {
    "lang": "en",
    "ignoreCapitalization": true,
    "excludeFiles": [
      "README-*.md"
    ],
    "ignoreText": [
      " \\(by [^)]+\\)."
    ],
    "dictionary": [
      "Async",
      "asyncGenerator",
      "CLI",
      "Clojure",
      "ClojureScript",
      "Crypto",
      "cryptographically",
      "formatAsync",
      "generateAsync",
      "gzip",
      "gzipped",
      "Haskell",
      "js",
      "JSDoc",
      "nanoid",
      "Nim",
      "nonSecure",
      "npm",
      "UUID",
      "v4",
      "Versioning",
      "JS",
      "PouchDB",
      "CouchDB",
      "async",
      "Tidelift"
    ]
  },
  "sharec": {
    "version": "0.4.0"
  }
}
